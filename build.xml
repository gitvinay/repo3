<project name="add"  default="compile">
<target name="makedir">
<mkdir dir="/build/class"/>
</target>
<target name="compile" depends="makedir">
<javac srcdir="./src" destdir="/build/class"/>
</target>
<target name="jar" depends="compile">
<jar destdir="/build/jar src=/build/class"/>
</target>
</project>

